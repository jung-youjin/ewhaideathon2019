<!-- Wrapper -->
	<div id="wrapper">
		
			<!--여기 배치좀 도와주세요ㅠㅠ-->
		<%=form_tag("/home/create", method:"post", multipart: true) do%>
			<%= label_tag(:title, "아이디어 이름: ")%>
			<%= text_field_tag (:title)%>
			<%= label_tag(:content, "아이디어 상세설명: ")%>
			<%= text_area_tag (:content)%>
			<%= label_tag('image_up',"아이디어 대표 이미지")%>
			<%= file_field_tag 'image_up'%>
			<%= submit_tag("등록완료")%>
		<%end%>
			
	<!-- Footer -->
		<footer id="footer">
			<p class="copyright">&copy; EwhaLikelion. Design: <a href="https://html5up.net">HTML5 UP</a>.</p>
		</footer>
			
	</div>		
<!-- BG -->
	<div id="bg"></div>